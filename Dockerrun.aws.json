{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "api",
      "image": "souravmondal10/api",
      "hostname": "api",
      "essential": false,
      "memory": 128
    },
    {
      "name": "frontendapp",
      "image": "souravmondal10/frontend",
      "hostname": "frontendapp",
      "essential": false,
      "memory": 128
    },
    {
      "name": "datastore",
      "image": "souravmondal10/datastore",
      "hostname": "datastore",
      "essential": false,
      "memory": 128
    },
    {
      "name": "nginxroute",
      "image": "souravmondal10/nginxroute",
      "hostname": "nginxroute",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": ["nginxroute","api", "frontend"],
      "memory": 128
    }
  ]
}
